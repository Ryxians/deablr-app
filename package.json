{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot server/index.ts",
    "start": "NODE_ENV=production bun server/index.ts",
    "build:bundle": "bun build --target=bun --production --outdir ./dist ./server/index.ts",
    "build:compile": "bun build --production --compile --outfile app ./server/index.ts",
    "run:prod": "NODE_ENV=production bun dist/index.js"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-router": "^1.136.6",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "prettier": "^3.6.2",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
